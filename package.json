    "type-check": "tsc --noEmit",
    "clean": "expo start --clear",
    "test": "echo \"No tests specified\" && exit 0",
    "doctor": "expo doctor",
    "install:clean": "rm -rf node_modules package-lock.json && npm install"
  },
